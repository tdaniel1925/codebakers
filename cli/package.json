{
  "name": "@codebakers/cli",
  "version": "3.3.12",
  "description": "CodeBakers CLI - Production patterns for AI-assisted development",
  "main": "dist/index.js",
  "bin": {
    "cli": "./dist/index.js",
    "codebakers": "./dist/index.js",
    "codebakers-mcp": "./dist/mcp/server.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
