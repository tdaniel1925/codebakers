  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^2.1.9",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^2.1.9"
  }
}
